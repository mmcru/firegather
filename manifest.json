{

  "manifest_version": 2,
  "name": "FireGather",
  "description": "Looks up MTG cards on Gatherer",
  "version": "1.0",
  "default_locale": "en",
  "applications": {
    "gecko": {
      "strict_min_version": "57.0"
    }
  },

  "background": {
    "scripts": ["cards.js", "lookup.js"]
  },
  
  "permissions": [
    "menus"
  ],
  
  "icons": {
    "64": "icons/64.png"
  }
}
